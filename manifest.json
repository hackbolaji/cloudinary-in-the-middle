{
	"manifest_version": 2,
	"name": "Cloudinary Extension",
	"version": "1.0",

	"description": "No description defined.",

	"icons": {
		"48": "icons/border-48.png"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["ext.js"]
		}
	],

	"permissions": ["webRequest", "<all_urls>", "webRequestBlocking", "storage"],

	"background": {
		"scripts": ["background.js"]
	},

	"options_ui": {
		"page": "options.html"
	},

	"applications": {
		"gecko": {
			"id": "junctionxvienna21019-cloudinary-extension@example.com"
		}
	}
}
