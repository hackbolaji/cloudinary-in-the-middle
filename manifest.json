{
	"manifest_version": 2,
	"name": "JunctionX Vienna 2019 Prototype",
	"version": "0.0.1",

	"description": "MITM extension for Cloudinary.",

	"icons": {
		"48": "icons/border-48.png"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["ext.js"]
		}
	],

	"permissions": ["webRequest", "<all_urls>", "webRequestBlocking", "storage"],

	"background": {
		"scripts": [
			"check-for-chrome.js",
			"node_modules/webextension-polyfill/dist/browser-polyfill.js",
			"background.js"
		]
	},

	"options_ui": {
		"page": "options.html"
	},

	"applications": {
		"gecko": {
			"id": "junctionxvienna21019-cloudinary-extension@example.com"
		}
	}
}
